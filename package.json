{
  "name": "json-html-report",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "start": "npm build && node server",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "jest": "vue-jest"
  },
  "dependencies": {
    "buefy": "^0.6.6",
